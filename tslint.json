{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens":false,
        "semicolon": false,
        "no-console": [false],
        "quotemark": [true, "single"],
        "prettier": [
            true,
            {
                "singleQuote": true,
                "semi": false,
                "trailingComma": "all",
                "printWidth": 80
            }
        ]
    },
    "rulesDirectory": []
}